{
  "name": "foyir-mono",
  "private": true,
  "engines": {
    "node": "14.18.1"
  },
  "scripts": {
    "install": "npm i && npx lerna bootstrap --no-ci",
    "build": "npx lerna run build",
    "start": "npm run build && npm run start:prod",
    "start:dev": "npx lerna run --parallel start:dev",
    "start:prod": "npx lerna run start:prod",
    "deploy:dev": "eb deploy foyir-mono-development"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "lerna": "^4.0.0"
  }
}
